   "source": [
    "#if a year is leap year or not\n",
    "year= int(input('enter year= '))\n",
    "if year % 4==0 and year%100!=0:\n",
    "    print('{} is leap year'.format(year))\n",
    "elif year % 400==0:\n",
    "    print('{} is leap year'.format(year))\n",
    "else:\n",
    "    print('{} is not leap year'.format (year))"
   ]
