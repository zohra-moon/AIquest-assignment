   "source": [
    "#if a number is prime or not\n",
    "N=int(input('Enter the number:'))\n",
    "for i in range(2,N):\n",
    "    if N%i==0:\n",
    "        print('The number',N, 'is not prime')\n",
    "        break\n",
    "else:\n",
    "        print('The number', N, 'is prime')"
   ]
